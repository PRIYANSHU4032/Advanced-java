package deloitte;

import java.util.Scanner;

public class reg {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the value");
		int num = sc.nextInt();
		while(num >0) {
			int ld = num%10;
			System.out.print(ld);
			num = num/10;
		}
		System.out.println();
	}

}
